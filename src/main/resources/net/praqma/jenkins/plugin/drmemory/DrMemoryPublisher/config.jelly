<j:jelly xmlns:j="jelly:core" xmlns:st="jelly:stapler" xmlns:d="jelly:define" xmlns:l="/lib/layout"
         xmlns:t="/lib/hudson" xmlns:f="/lib/form">

    <f:entry title="${%Dr Memory result path}">
        <f:textbox name="drmemory.logPath" value="${instance.logPath}" default="drmemory/" />
    </f:entry>
    
    <f:entry title="Show total leaks" help="">
        <f:checkbox name="graph.total-leaks" checked="${instance.totalLeaks}" />
    </f:entry>
    
</j:jelly>